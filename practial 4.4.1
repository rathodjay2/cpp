
#include<iostream>
using namespace std;

int max(int x, int y) {
    return (x > y) ? x : y;
}

int max(int x, int y, int z) {
    return max(max(x, y), z);
}

int main() {
    int num1, num2, num3;
    cout << "Enter three numbers: ";
    cin >> num1 >> num2 >> num3;

    cout << "The maximum number is: " << max(num1, num2, num3) << endl;
    return 0;
}
