#include<iostream>
using namespace std;

int main()
{
    int b[100],n,i,j,temp;
    cout<<"Enter The Limit : ";
    cin>>n;
 
    cout<<"Enter "<<n<<" Value :"<<endl;
    for(i=0;i<n;i++)
    {
        cin>>b[i];
    }
 
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(b[i]>b[j])
            {
                temp=b[i];
                b[i]=b[j];
                b[j]=temp;
            }
        }
    }
 
    cout<<"Sorted Array : "<<endl;
    for(i=0;i<n;i++)
    {
        cout<<b[i]<<endl;
    }
    return 0;
}
